public class Main
{
	public static void main(String[] args) {
		System.out.println("main activated");
		if(!(!true))
		    System.out.println("It is true");
		    else
		System.out.println("It is not true");
		
		if(!(!true || true))
		    System.out.println("Printing values in true");
		else
		System.out.println(" It is not true anymore...");
	}
}
